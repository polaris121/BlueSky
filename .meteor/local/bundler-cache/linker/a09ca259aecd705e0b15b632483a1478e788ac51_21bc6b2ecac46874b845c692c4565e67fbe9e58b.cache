return (\r\n    <div className=\"app-container\">\r\n      <Header onHomeClick={handleHomeClick} onContactClick={handleContactClick} />\r\n      <div className=\"main-container\">\r\n        <Sidebar onAdminClick={handleAdminClick} />\r\n        {currentContent}\r\n      </div>\r\n      <Footer />\r\n    </div>\r\n  );\r\n};\r\n","import React from 'react';\r\n\r\nexport const ContactInfo = () => (\r\n  <section>\r\n    <h2>Contact Us</h2>\r\n    <p>\r\n      You can reach us at <a href=\"mailto:test@example.com\">test@example.com</a>\r\n    </p>\r\n  </section>\r\n);\r\n","import React from 'react';\r\n\r\nexport const Footer = () => (\r\n  <footer>\r\n    <p>&copy; 2025 My Meteor App</p>\r\n  </footer>\r\n);\r\n","import React from 'react';\r\n\r\nexport const Header = ({ onHomeClick, onContactClick }) => (\r\n  <header className=\"header\">\r\n    <div className=\"logo\">Logo</div>\r\n    <nav>\r\n      <a href=\"#\" className=\"sidebar-button\" onClick={(e) => { e.preventDefault(); onHomeClick(); }}>Home</a>\r\n      <a href=\"#\" className=\"sidebar-button\">About</a>\r\n      <a href=\"#\" className=\"sidebar-button\" onClick={(e) => { e.preventDefault(); onContactClick(); }}>Contact</a>\r\n    </nav>\r\n    <div className=\"user-profile\">\r\n      <input type=\"search\" placeholder=\"Search...\" />\r\n      <span>&#128100;</span>\r\n    </div>\r\n  </header>\r\n);\r\n","import React from 'react';\r\n\r\nexport const MainContent = () => (\r\n  <main className=\"main-content\">\r\n    <article>\r\n      <h1 className=\"welcome-title\">Welcome to the Pool</h1>\r\n      <p className=\"welcome-subtitle\">Where Luck and Strategy Meets!</p>\r\n      <p className=\"welcome-tagline\">-- 99% Dump Luck and 1% Strategy --</p>\r\n    </article>\r\n  </main>\r\n);\r\n","import React from 'react';\r\n\r\nexport const Sidebar = ({ onAdminClick }) => (\r\n  <aside className=\"sidebar\">\r\n    <ul>\r\n      <li><a href=\"#\" className=\"sidebar-button\" onClick={(e) => { e.preventDefault(); onAdminClick(); }}>Admin</a></li>\r\n      <li><a href=\"#\" className=\"sidebar-button\">About</a></li>\r\n      <li><a href=\"#\" className=\"sidebar-button\">Services</a></li>\r\n      <li><a href=\"#\" className=\"sidebar-button\">Contact</a></li>\r\n    </ul>\r\n  </aside>\r\n);\r\n","import React from 'react';\r\nimport { createRoot } from 'react-dom/client';\r\nimport { Meteor } from 'meteor/meteor';\r\nimport { App } from '/imports/ui/App';\r\n\r\nMeteor.startup(() => {\r\n  const container = document.getElementById('react-target');\r\n  const root = createRoot(container);\r\n  root.render(<App />);\r\n});\r\n"]}}]