AAC,aAAA;IAAGC,SAAS,EAAC;EAAiB,GAAC,qCAAsC,CAC9D,CACL,CACP;EAACiB,EAAA,GARWY,WAAW;EAAA,IAAAZ,EAAA;EAAAC,YAAA,CAAAD,EAAA;AAAA,EAAAE,IAAA,OAAAC,MAAA,E;;;;;;;;;;;;;;;;;;;;;;ECAjB,MAAMa,OAAO,GAAGI,IAAA;IAAA,IAAC;MAAEH;IAAa,CAAC,GAAAG,IAAA;IAAA,oBACtCvC,KAAA,CAAAC,aAAA;MAAOC,SAAS,EAAC;IAAS,gBACxBF,KAAA,CAAAC,aAAA,0BACED,KAAA,CAAAC,aAAA,0BAAID,KAAA,CAAAC,aAAA;MAAGqC,IAAI,EAAC,GAAG;MAACpC,SAAS,EAAC,gBAAgB;MAACsC,OAAO,EAAGtD,CAAC,IAAK;QAAEA,CAAC,CAACC,cAAc,CAAC,CAAC;QAAEiD,YAAY,CAAC,CAAC;MAAE;IAAE,GAAC,OAAQ,CAAK,CAAC,eAClHpC,KAAA,CAAAC,aAAA,0BAAID,KAAA,CAAAC,aAAA;MAAGqC,IAAI,EAAC,GAAG;MAACpC,SAAS,EAAC;IAAgB,GAAC,OAAQ,CAAK,CAAC,eACzDF,KAAA,CAAAC,aAAA,0BAAID,KAAA,CAAAC,aAAA;MAAGqC,IAAI,EAAC,GAAG;MAACpC,SAAS,EAAC;IAAgB,GAAC,UAAW,CAAK,CAAC,eAC5DF,KAAA,CAAAC,aAAA,0BAAID,KAAA,CAAAC,aAAA;MAAGqC,IAAI,EAAC,GAAG;MAACpC,SAAS,EAAC;IAAgB,GAAC,SAAU,CAAK,CACxD,CACC,CACR;EAAA;EAACiB,EAAA,GATWgB,OAAO;EAAA,IAAAhB,EAAA;EAAAC,YAAA,CAAAD,EAAA;AAAA,EAAAE,IAAA,OAAAC,MAAA,E;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;ECGpBmB,MAAM,CAACC,OAAO,CAAC,MAAM;IACnB,MAAMC,SAAS,GAAGC,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC;IACzD,MAAMC,IAAI,GAAGC,UAAU,CAACJ,SAAS,CAAC;IAClCG,IAAI,CAACE,MAAM,cAAChD,KAAA,CAAAC,aAAA,CAACsB,GAAG,OAAG,CAAC;EACtB,CAAC,CAAC;AAAA,EAAAF,IAAA,OAAAC,MAAA,E","file":"/app.js","sourcesContent":["import React, { useState } from 'react';\r\n\r\nexport const AdminPanel = () => {\r\n  const [users, setUsers] = useState([]);\r\n  const [newUsername, setNewUsername] = useState('');\r\n  const [newPassword, setNewPassword] = useState('');\r\n  const [deleteUsername, setDeleteUsername] = useState('');\r\n\r\n  const handleAddUser = (e) => {\r\n    e.preventDefault();\r\n    if (newUsername.trim() === '') {\r\n      alert('Please enter a username.');\r\n      return;\r\n    }\r\n    const newUser = {\r\n      id: Date.now(), // simple unique id\r\n      username: newUsername,\r\n      role: 'User',\r\n    };\r\n    setUsers([...users, newUser]);\r\n    setNewUsername('');\r\n    setNewPassword('');\r\n  };\r\n\r\n  const handleDeleteUser = (e) => {\r\n    e.preventDefault();\r\n    if (deleteUsername.trim() === '') {\r\n      alert('Please enter a username to delete.');\r\n      return;\r\n    }\r\n    if (!users.find(user => user.username === deleteUsername)) {\r\n      alert('User not found.');\r\n      return;\r\n    }\r\n    setUsers(users.filter(user => user.username !== deleteUsername));\r\n    setDeleteUsername('');\r\n  };\r\n\r\n  return (\r\n    <div className=\"admin-panel\">\r\n      <div className=\"admin-header\">\r\n        <h1>Welcome to the Admin Panel</h1>\r\n        <p className=\"admin-user-role\">User as Admin</p>\r\n      </div>\r\n      <div className=\"admin-content\">\r\n        <div className=\"admin-section\">\r\n          <form className=\"admin-form horizontal-form\" onSubmit={handleAddUser}>\r\n            <div className=\"form-group\">\r\n              <label htmlFor=\"username\">User Name</label>\r\n              <input \r\n                type=\"text\" \r\n                id=\"username\" \r\n                name=\"username\" \r\n                placeholder=\"Enter new user's name\" \r\n                value={newUsername}\r\n                onChange={(e) => setNewUsername(e.target.value)}\r\n              />\r\n            </div>\r\n            <div className=\"form-group\">\r\n              <label htmlFor=\"password\">Password</label>\r\n              <input \r\n                type=\"password\" \r\n                id=\"password\" \r\n                name=\"password\" \r\n                placeholder=\"Enter password\" \r\n                value={newPassword}\r\n                onChange={(e) => setNewPassword(e.target.value)}\r\n              />\r\n            </div>\r\n            <button type=\"submit\" className=\"admin-button\">Add User</button>\r\n          </form>\r\n        </div>\r\n\r\n        <div className=\"admin-section\">\r\n          <form className=\"admin-form horizontal-form\" onSubmit={handleDeleteUser}>\r\n            <div className=\"form-group\">\r\n              <label htmlFor=\"delete-username\">User Name</label>\r\n              <input \r\n                type=\"text\" \r\n                id=\"delete-username\" \r\n                name=\"delete-username\" \r\n                placeholder=\"Enter user's name to delete\" \r\n                value={deleteUsername}\r\n                onChange={(e) => setDeleteUsername(e.target.value)}\r\n              />\r\n            </div>\r\n            <button type=\"submit\" className=\"admin-button danger\">Delete User</button>\r\n          </form>\r\n        </div>\r\n\r\n        <div className=\"admin-section\">\r\n          <h2>User List</h2>\r\n          <table className=\"users-table\">\r\n            <thead>\r\n              <tr>\r\n                <th>User Name</th>\r\n                <th>Role</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              {users.length > 0 ? (\r\n                users.map(user => (\r\n                  <tr key={user.id}>\r\n                    <td>{user.username}</td>\r\n                    <td>{user.role}</td>\r\n                  </tr>\r\n                ))\r\n              ) : (\r\n                <tr>\r\n                  <td colSpan=\"2\" style={{ textAlign: 'center', fontStyle: 'italic', color: 'rgba(255, 255, 255, 0.5)' }}>No users found.</td>\r\n                </tr>\r\n              )}\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n","import React, { useState } from 'react';\r\nimport { Header } from './Header.jsx';\r\nimport { Footer } from './Footer.jsx';\r\nimport { Sidebar } from './Sidebar.jsx';\r\nimport { MainContent } from './MainContent.jsx';\r\nimport { ContactInfo } from './ContactInfo.jsx';\r\nimport { AdminPanel } from './AdminPanel.jsx';\r\n\r\nexport const App = () => {\r\n  const [content, setContent] = useState('main');\r\n\r\n  const handleHomeClick = () => {\r\n    setContent('main');\r\n  };\r\n\r\n  const handleContactClick = () => {\r\n    setContent('contact');\r\n  };\r\n\r\n  const handleAdminClick = () => {\r\n    setContent('admin');\r\n  };\r\n\r\n  let currentContent;\r\n  if (content === 'contact') {\r\n    currentContent = <ContactInfo />;\r\n  } else if (content === 'admin') {\r\n    currentContent = <AdminPanel />;\r\n  } else {\r\n    currentContent = <MainContent />;\r\n  }\r\n\r\n  return (\r\n    <div className=\"app-container\">\r\n      <Header onHomeClick={handleHomeClick} onContactClick={handleContactClick} />\r\n      <div className=\"main-container\">\r\n        <Sidebar onAdminClick={handleAdminClick} />\r\n        {currentContent}\r\n      </div>\r\n      <Footer />\r\n    </div>\r\n  );\r\n};\r\n","import React from 'react';\r\n\r\nexport const ContactInfo = () => (\r\n  <section>\r\n    <h2>Contact Us</h2>\r\n    <p>\r\n      You can reach us at <a href=\"mailto:test@example.com\">test@example.com</a>\r\n    </p>\r\n  </section>\r\n);\r\n","import React from 'react';\r\n\r\nexport const Footer = () => (\r\n  <footer>\r\n    <p>&copy; 2025 My Meteor App</p>\r\n  </footer>\r\n);\r\n","import React from 'react';\r\n\r\nexport const Header = ({ onHomeClick, onContactClick }) => (\r\n  <header className=\"header\">\r\n    <div className=\"logo\">Logo</div>\r\n    <nav>\r\n      <a href=\"#\" className=\"sidebar-button\" onClick={(e) => { e.preventDefault(); onHomeClick(); }}>Home</a>\r\n      <a href=\"#\" className=\"sidebar-button\">About</a>\r\n      <a href=\"#\" className=\"sidebar-button\" onClick={(e) => { e.preventDefault(); onContactClick(); }}>Contact</a>\r\n    </nav>\r\n    <div className=\"user-profile\">\r\n      <input type=\"search\" placeholder=\"Search...\" />\r\n      <span>&#128100;</span>\r\n    </div>\r\n  </header>\r\n);\r\n","import React from 'react';\r\n\r\nexport const MainContent = () => (\r\n  <main className=\"main-content\">\r\n    <article>\r\n      <h1 className=\"welcome-title\">Welcome to the Pool</h1>\r\n      <p className=\"welcome-subtitle\">Where Luck and Strategy Meets!</p>\r\n      <p className=\"welcome-tagline\">-- 99% Dump Luck and 1% Strategy --</p>\r\n    </article>\r\n  </main>\r\n);\r\n","import React from 'react';\r\n\r\nexport const Sidebar = ({ onAdminClick }) => (\r\n  <aside className=\"sidebar\">\r\n    <ul>\r\n      <li><a href=\"#\" className=\"sidebar-button\" onClick={(e) => { e.preventDefault(); onAdminClick(); }}>Admin</a></li>\r\n      <li><a href=\"#\" className=\"sidebar-button\">About</a></li>\r\n      <li><a href=\"#\" className=\"sidebar-button\">Services</a></li>\r\n      <li><a href=\"#\" className=\"sidebar-button\">Contact</a></li>\r\n    </ul>\r\n  </aside>\r\n);\r\n","import React from 'react';\r\nimport { createRoot } from 'react-dom/client';\r\nimport { Meteor } from 'meteor/meteor';\r\nimport { App } from '/imports/ui/App';\r\n\r\nMeteor.startup(() => {\r\n  const container = document.getElementById('react-target');\r\n  const root = createRoot(container);\r\n  root.render(<App />);\r\n});\r\n"]}}]